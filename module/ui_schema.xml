<?xml version="1.0"?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Fill This In</h:title>
    <model>
      <instance>
        <faims id="Fill_This_In">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <User>
            <User>
              <Users/>
            </User>
          </User>
          <Control>
            <Control>
              <Next_ID/>
              <Create_New_Record/>
            </Control>
            <Search>
              <Colgroup_0>
                <Col_0>
                  <Search_Term/>
                </Col_0>
                <Col_1>
                  <Search_Button/>
                </Col_1>
              </Colgroup_0>
              <Entity_List/>
            </Search>
          </Control>
          <Scan_Record>
            <Tab>
              <ID/>
              <Attach_New_Files/>
              <File_Type/>
              <Language/>
              <Repository/>
              <Garrison/>
              <Period_From/>
              <Period_Till/>
              <Doc_Name/>
              <File_Reference/>
              <Theme_Architecture_Space/>
              <Theme_Crime_Punishment/>
              <Theme_Economics/>
              <Theme_Education/>
              <Theme_Gender/>
              <Theme_Host_Communities/>
              <Theme_Legacy_Remembering/>
              <Theme_Media/>
              <Theme_Medical/>
              <Theme_Military/>
              <Theme_Politics/>
              <Theme_Race/>
              <Theme_Social/>
            </Tab>
            <Notes>
              <Notes/>
            </Notes>
            <Files>
              <Files/>
              <Button_Files/>
            </Files>
          </Scan_Record>
          <logic/>
        </faims>
      </instance>
      <bind type="decimal" nodeset="/faims/Control/Control/Next_ID"/>
      <bind type="decimal" nodeset="/faims/Scan_Record/Tab/ID"/>
      <bind type="date" nodeset="/faims/Scan_Record/Tab/Period_From"/>
      <bind type="date" nodeset="/faims/Scan_Record/Tab/Period_Till"/>
    </model>
  </h:head>
  <h:body>
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <group ref="User">
      <label>{User}</label>
      <group ref="User" faims_scrollable="false">
        <label>{User}</label>
        <select1 appearance="compact" ref="Users">
          <label>{Users}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="Control">
      <label>{Control}</label>
      <group ref="Control">
        <label>{Control}</label>
        <input ref="Next_ID" faims_style_class="required">
          <label>{Next_ID}</label>
        </input>
        <trigger ref="Create_New_Record">
          <label>{Create_New_Record}</label>
        </trigger>
      </group>
      <group ref="Search" faims_scrollable="false">
        <label>{search}</label>
        <group faims_style="orientation" ref="Colgroup_0">
          <label/>
          <group faims_style="even" ref="Col_0">
            <label/>
            <input ref="Search_Term">
              <label>{Search_Term}</label>
            </input>
          </group>
          <group faims_style="large" ref="Col_1">
            <label/>
            <trigger ref="Search_Button">
              <label>{Search}</label>
            </trigger>
          </group>
        </group>
        <select1 appearance="compact" ref="Entity_List">
          <label>{Entity_List}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="Scan_Record" faims_archent_type="Scan Record">
      <label>{Scan_Record}</label>
      <group ref="Tab">
        <label>{Metadata}</label>
        <input faims_attribute_type="measure" ref="ID" faims_attribute_name="ID" faims_style_class="required">
          <label>{ID}</label>
        </input>
        <trigger ref="Attach_New_Files">
          <label>{Attach_New_Files}</label>
        </trigger>
        <select1 faims_attribute_type="vocab" ref="File_Type" faims_attribute_name="File Type">
          <label>{File_Type}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 faims_attribute_type="vocab" ref="Language" faims_attribute_name="Language">
          <label>{Language}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 faims_attribute_type="vocab" ref="Repository" faims_attribute_name="Repository">
          <label>{Repository}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 faims_attribute_type="vocab" ref="Garrison" faims_attribute_name="Garrison">
          <label>{Garrison}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <input faims_attribute_type="measure" ref="Period_From" faims_attribute_name="Period From">
          <label>{Date_from}</label>
        </input>
        <input faims_attribute_type="measure" ref="Period_Till" faims_attribute_name="Period Till">
          <label>{Date_to}</label>
        </input>
        <input faims_attribute_type="measure" ref="Doc_Name" faims_attribute_name="Doc Name">
          <label>{Doc_Name}</label>
        </input>
        <input faims_attribute_type="measure" ref="File_Reference" faims_attribute_name="File Reference">
          <label>{File_Reference}</label>
        </input>
        <select faims_attribute_type="vocab" ref="Theme_Architecture_Space" faims_attribute_name="Theme Architecture Space">
          <label>{Theme__Architecture_Space}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select faims_attribute_type="vocab" ref="Theme_Crime_Punishment" faims_attribute_name="Theme Crime Punishment">
          <label>{Theme__Crime_Punishment}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select faims_attribute_type="vocab" ref="Theme_Economics" faims_attribute_name="Theme Economics">
          <label>{Theme__Economics}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select faims_attribute_type="vocab" ref="Theme_Education" faims_attribute_name="Theme Education">
          <label>{Theme__Education}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select faims_attribute_type="vocab" ref="Theme_Gender" faims_attribute_name="Theme Gender">
          <label>{Theme__Gender}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select faims_attribute_type="vocab" ref="Theme_Host_Communities" faims_attribute_name="Theme Host Communities">
          <label>{Theme__Host_Communities}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select faims_attribute_type="vocab" ref="Theme_Legacy_Remembering" faims_attribute_name="Theme Legacy Remembering">
          <label>{Theme__Legacy_Remembering}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select faims_attribute_type="vocab" ref="Theme_Media" faims_attribute_name="Theme Media">
          <label>{Theme__Media}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select faims_attribute_type="vocab" ref="Theme_Medical" faims_attribute_name="Theme Medical">
          <label>{Theme__Medical}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select faims_attribute_type="vocab" ref="Theme_Military" faims_attribute_name="Theme Military">
          <label>{Theme__Military}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select faims_attribute_type="vocab" ref="Theme_Politics" faims_attribute_name="Theme Politics">
          <label>{Theme__Politics}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select faims_attribute_type="vocab" ref="Theme_Race" faims_attribute_name="Theme Race">
          <label>{Theme__Race}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <select faims_attribute_type="vocab" ref="Theme_Social" faims_attribute_name="Theme Social">
          <label>{Theme__Social}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
      </group>
      <group ref="Notes">
        <label>{Notes}</label>
        <input faims_attribute_type="measure" ref="Notes" faims_attribute_name="Notes">
          <label>{Notes}</label>
        </input>
      </group>
      <group ref="Files">
        <label>{Files}</label>
        <select type="file" faims_attribute_type="measure" faims_sync="true" ref="Files" faims_attribute_name="Files">
          <label>{Files}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <trigger ref="Button_Files">
          <label>{Button_Files}</label>
        </trigger>
      </group>
    </group>
  </h:body>
</h:html>
